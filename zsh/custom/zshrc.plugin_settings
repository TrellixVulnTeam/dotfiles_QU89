# ZPLUG
export ZPLUG_FILTER="fzf"

#PyEnv
if [ -e $HOME/opt/pyenv ]; then
   export PYENV_HOME=$HOME/opt/pyenv
else
    export PYENV_HOME="/opt/local/bin/pyenv"
fi

# Enhancd
export ENHANCD_DOT_ARG=--
export ENHANCD_HOOK_AFTER_CD=ls
export ENHANCD_FILTER="fzf"

